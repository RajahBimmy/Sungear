extends layout

block content
    nav(class='navbar navbar-default')
        div(class='container-fluid')
            div(class='navbar-header')
                a(class='navbar-brand') #{title}
            div(class='collapse navbar-collapse')
                ul(class='nav navbar-nav')
                    li(class='dropdown')
                        a(href='#', class='dropdown-toggle', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') File
                        ul(class='dropdown-menu')
                            li
                                a(href='#') Load...
                            li
                                a(href='#') Screen Shot
                    li(class='dropdown')
                        a(href='#', class='dropdown-toggle', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') View
                        ul(class='dropdown-menu')
                            li
                                a(href='#') Genes
                                    span(class='glyphicon glyphicon-ok')
                            li
                                a(href='#') Controls
                                    span(class='glyphicon glyphicon-ok')
                            li
                                a(href='#') Sungear
                                    span(class='glyphicon glyphicon-ok')
                            li
                                a(href='#') GO Terms
                                    span(class='glyphicon glyphicon-ok')
                            li(role='separator', class='divider')
                            li
                                a(href='#') GeneLights
                                    span(class='glyphicon glyphicon-ok')
                            li(role='separator', class='divider')
                            li
                                a(href='#') Reposition
                                    span(class='glyphicon glyphicon-ok')
                            li
                                a(href='#') Relax Vessels
                                    span(class='glyphicon glyphicon-ok')
                            li(class='disabled')
                                a(href='#') Full Screen
                                    span(class='glyphicon glyphicon-ok')
                    li(class='dropdown')
                        a(href='#', class='dropdown-toggle', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') Help
                        ul(class='dropdown-menu')
                            li
                                a(href='#') About...
                            li
                                a(href='#') File Info...

    div(class='container')
        div(class='col-md-3')
            div(id='genes', class='img-rounded')
                h3 Genes
                table(class='table table-striped img-rounded', id='geneTable')

    script(src='/javascripts/out.js')
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js')
    script(src='http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js')